{
  "name": "feels",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@okta/okta-react": "^1.2.0",
    "@okta/okta-signin-widget": "^2.5.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "colors": "^1.1.2",
    "cross-env": "^5.1.3",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.16",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "popper.js": "^1.14.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.8",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "banners": "node tools/maintain-banners.js {okta-hosted-login,custom-login,tools,scripts}/{*js,src/**/*{js,jsx,css}}",
    "okta-hosted-login-server": "npm start --prefix okta-hosted-login/",
    "test:okta-hosted-login": "export TEST_TYPE=implicit || setx TEST_TYPE \"implicit\" && protractor okta-oidc-tck/e2e-tests/okta-hosted-login/conf.js",
    "custom-login-server": "npm start --prefix custom-login/",
    "test:custom-login": "export TEST_TYPE=implicit || setx TEST_TYPE \"implicit\" && protractor okta-oidc-tck/e2e-tests/custom-login/conf.js",
    "resource-server": "node samples-nodejs-express-4/resource-server/server.js",
    "test:e2e": "npm run test:okta-hosted-login && npm run test:custom-login",
    "pretest": "npm run webdriver-update && node scripts/setup-env.js",
    "pretest-legacy": "npm run webdriver-update-legacy && node scripts/setup-env.js",
    "webdriver-update": "webdriver-manager update --gecko false",
    "webdriver-update-legacy": "webdriver-manager update --versions.chrome 2.45 --gecko false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
