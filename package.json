    "@okta/okta-react": "^1.0.3",
    "@okta/okta-signin-widget": "^2.5.0",
    "@sentry/browser": "^4.6.4",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "moment": "^2.24.0",
